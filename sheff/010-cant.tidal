do
    let dens = 1
        melody = "{[0 4] 2 -24 ~ [ 4 11] ~ ~ [0] [14 16] ~ 11 ~ ~}%16"        
    p "synth" 
        $ id -- harmorOn 
        $ every 0 (stutWith (shiftBy 20 $ choose [2,3]) (0.125*6) (|+ note (shiftBy 44 $ choose [5,7,12,14,-12,-5,-7]))) 
        $ (|+ note "-7") 
        $ slow 4 $ note melody 
        # midichan 0 # gain 1.15 # cps 0.6666666 # s "harmor2"
    p "ccs" $ density dens $ stack [ 
        pan melody # ccv (range 0 127 $ slow 21.1 sine) # filter1
        , pan melody # ccv (range 127 127 $ slow 18.31 sine) # filter1width
        , pan melody # ccv (range 0 127 $ slow 20.25 sine) # filter1env
        , pan melody # ccv (range 70 127 $ slow 19.212 sine) # filter2
        , pan melody # ccv (range 0 127 $ slow 16.41 sine) # filter2width
        , pan melody # ccv (range 40 90 $ slow 17.587 sine) # filter2env
        , pan melody # ccv (range 20 127 $ slow 22.6801 sine) # uniDetune
        , pan melody # ccv (range 64 64 $ slow 15 tri) # porta
        ] # midichan 0 # s "harmor2"

    let 
        bd1 p = fast 2 $ whenmod 32 16 (# pad6n) $ shift $ sb 0.2 (within (0,0.125) g) $ struct "1(3,8,3)" $ p
        cp1 p = (0.125 <~) $ p
        sd1 p = (0.25 ~>) $ p 
        bd2 p = whenmod 32 16 (# pad6n) $ struct "1 1 1 [~ 1]" $ p
        sd2 p = off 0.125 id $ ((11/16) ~>) $ p
        cp2 p = (0.125 ~>) $ p
        bdpat = bd1
        sdpat = sd1
        cppat = cp1
    p "rytm"
        $ limit
        $ shift
        $ useIter (iter 16)
        $ reduce $ stack [
            kickOn $ bdpat $ pad1n
            , clapOn $ cppat $ pad4n
            , perc1On $ sdpat $ pad2n
            , hatOn 
              $ whenmod 16 8 (slow 2)
              $ whenmod 20 10 (slow (4/3))
              $ struct "1*16" $ pad11n
            , g $ padOn $ dg6 $ struct "1(<5 3 7>,16,<0 4 8 3 9>)" $ pad5n 
            , perf5 "0"
        ] # rytm

hush





do
  let dens = 1
      melody = "{[0 4] 2 -24 ~ [ 4 11] ~ ~ [0] [14 16] ~ 11 ~ ~}%16"        
  p "synth" 
      $ harmorOn $ sometimesBy 0 (stutWith (shiftBy 20 $ choose [1,2,3]) (0.125*3) (|+ note (shiftBy 44 $ choose [5,7,12,14]))) 
      $ (|+ note "-7") 
      $ slow 4 $ note melody 
      # cantChan # gain 1.15 # cps 0.6666666 # s "harmor"
  d1
    $ limit
    $ stack [
      fast 2 $ struct "1(3,8)" $ midichan "<0>" # note "c3"
    ] # s "rytm" # cps (150/120/2)

    hush




